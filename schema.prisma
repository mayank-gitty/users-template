// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.myprisma/client"
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  company   Company?  @relation("User_company", fields: [companyId], references: [id])
  companyId String?   @map("company")
  createdAt DateTime? @default(now())
  role      String?

  @@index([companyId])
}

model MultipleUser {
  id           String  @id @default(cuid())
  name         String  @default("")
  mobilenumber String  @default("")
  email        String  @default("")
  address      String  @default("")
  role         String?
}

model ProfileUser {
  id                  String     @id @default(cuid())
  resume_headline     String     @default("")
  itskills            ItSkill[]  @relation("ProfileUser_itskills")
  education           String     @default("")
  keyskills           KeySkill[] @relation("ProfileUser_keyskills")
  total_experience    String     @default("")
  relevent_experience String     @default("")
  profile_summary     String     @default("")
}

model Education {
  id                          String         @id @default(cuid())
  education                   AddEducation[] @relation("Education_education")
  university                  String         @default("")
  course                      String         @default("")
  specialization              String         @default("")
  courseDuration              String         @default("")
  gradingSystem               String         @default("")
  marks                       String         @default("")
  add_doctorate_phd           String         @default("")
  add_masters_post_gradiation String         @default("")
  add_graduation_diploma      String         @default("")
  add_class_X11               String         @default("")
  add_class_X                 String         @default("")
  add_below_10th              String         @default("")
}

model ItSkill {
  id                        String        @id @default(cuid())
  name                      String        @default("")
  version                   String        @default("")
  lastUsed                  String        @default("")
  experience                String        @default("")
  from_ProfileUser_itskills ProfileUser[] @relation("ProfileUser_itskills")
}

model KeySkill {
  id                         String        @id @default(cuid())
  name                       String        @default("")
  version                    String        @default("")
  lastUsed                   String        @default("")
  experience                 String        @default("")
  from_ProfileUser_keyskills ProfileUser[] @relation("ProfileUser_keyskills")
}

model Project {
  id                   String  @id @default(cuid())
  project_Title        String  @default("")
  client               String  @default("")
  project_status       String?
  duration             String  @default("")
  details_of_project   String  @default("")
  project_site         String?
  Nature_of_employment String?
  Team_Size            String  @default("")
  Role                 String  @default("")
  Role_Description     String  @default("")
  Skills_Used          String  @default("")
}

model Company {
  id                String @id @default(cuid())
  name              String @default("")
  from_User_company User[] @relation("User_company")
}

model AddEducation {
  id                       String      @id @default(cuid())
  name                     String      @default("")
  education                String      @default("")
  course                   String      @default("")
  university               String      @default("")
  course_type              String?
  specialization           String      @default("")
  courseDuration           String      @default("")
  gradingSystem            String      @default("")
  marks                    String      @default("")
  from_Education_education Education[] @relation("Education_education")
}
